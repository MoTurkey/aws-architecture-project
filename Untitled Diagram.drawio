<mxfile>
  <diagram name="AWS Architecture">
    <mxGraphModel>
      <root>
        <!-- CloudFormation Block -->
        <mxCell id="1" value="CloudFormation" style="rounded=1;fillColor=#FFD966;strokeColor=#333333;shadow=1;" vertex="1" parent="0">
          <mxGeometry x="200" y="20" width="120" height="50" as="geometry"/>
        </mxCell>
        
        <!-- S3 Bucket Block -->
        <mxCell id="2" value="S3 Bucket" style="rounded=1;fillColor=#DAE8FC;strokeColor=#333333;shadow=1;" vertex="1" parent="0">
          <mxGeometry x="50" y="150" width="120" height="50" as="geometry"/>
        </mxCell>
        
        <!-- EC2 Instance Block -->
        <mxCell id="3" value="EC2 Instance" style="rounded=1;fillColor=#D5E8D4;strokeColor=#333333;shadow=1;" vertex="1" parent="0">
          <mxGeometry x="350" y="150" width="120" height="50" as="geometry"/>
        </mxCell>
        
        <!-- Arrows and Connections -->
        <!-- CloudFormation to S3 Bucket -->
        <mxCell id="4" edge="1" source="1" target="2" style="edgeStyle=orthogonalEdgeStyle;strokeColor=#333333;">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>
        
        <!-- CloudFormation to EC2 Instance -->
        <mxCell id="5" edge="1" source="1" target="3" style="edgeStyle=orthogonalEdgeStyle;strokeColor=#333333;">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>
        
        <!-- S3 to EC2 (optional data flow) -->
        <mxCell id="6" value="Data flow" edge="1" source="2" target="3" style="edgeStyle=orthogonalEdgeStyle;dashed=1;strokeColor=#333333;">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

      </root>
    </mxGraphModel>
  </diagram>
</mxfile>
